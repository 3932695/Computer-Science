#include <iostream>

using namespace std;


void printArray(int[] array, int numElements){
     for (int i = numElements - 1; i--){
     	 cout << array[i];
     }  
}

void pascal(int rows){
     int array[rows];
     for(int i = 0; i < rows; i++){
     	     int row[i+1];
	     for(int j = 0; j < (i+1); j++){
\\	     	     row[j] = pascalc(i, j);
		     row[j] = 0;
	     }
	     printArray(row, i+1);
     }
}

